{
	"name": "taskflow",
	"displayName": "TaskFlow",
	"description": "Execute markdown tasks with AI agents",
	"version": "0.1.0",
	"publisher": "taskflow",
	"repository": {
		"type": "git",
		"url": "https://github.com/bk-bf/taskflow.git"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "taskflow.startTask",
				"title": "Start Task"
			},
			{
				"command": "taskflow.retryTask",
				"title": "Retry Task"
			},
			{
				"command": "taskflow.test",
				"title": "TaskFlow: Test"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"test": "npm run compile && mocha out/test/**/*.test.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.74.0",
		"@types/node": "16.x",
		"@types/mocha": "^10.0.0",
		"typescript": "^4.9.4",
		"mocha": "^10.0.0"
	},
	"dependencies": {
		"markdown-it": "^13.0.1"
	}
}